# Bambu Lab Access Token (JWT from login)
BAMBU_TOKEN=your-bambu-lab-jwt-token-here

# API Key for SCUBE to authenticate (generate a random string)
SCUBE_API_KEY=your-secret-api-key-here

# Port (Railway/Render will set this automatically)
PORT=5000

# MQTT session duration in seconds (how long to keep connections alive)
MQTT_SESSION_DURATION=120
